{
  "version": 2,
  "name": "aevalmori",
  "builds": [
    { "src": "select-locale.js", "use": "@now/node-server" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/", "dest": "select-locale.js" },
    { "src": "/fr/?", "dest": "/?locale=fr" },
    { "src": "/it/?", "dest": "/?locale=it" },
    { "src": "/fr/qui-suis-je", "dest": "/about?locale=fr" },
    { "src": "/it/chi-sono", "dest": "/about?locale=it" },
    { "src": "/fr/consultations-individuelles", "dest": "/services-individuals?locale=fr" },
    { "src": "/it/consultazioni-individuali", "dest": "/services-individuals?locale=it" },
    { "src": "/fr/constellations-familiales", "dest": "/services-constellations?locale=fr" },
    { "src": "/it/costellazioni-familiari", "dest": "/services-constellations?locale=it" },
    { "src": "/fr/dmoka", "dest": "/services-dmoka?locale=fr" },
    { "src": "/it/dmoka", "dest": "/services-dmoka?locale=it" },
    { "src": "/fr/temoignages", "dest": "/testimonies?locale=fr" },
    { "src": "/it/testimonianze", "dest": "/testimonies?locale=it" },
    { "src": "/fr/contact", "dest": "/contact?locale=fr" },
    { "src": "/it/contatto", "dest": "/contact?locale=it" }
  ]
}